<p id="notice"><%= notice %></p>

<h1>Plans</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>User</th>
      <th>List</th>
      <th>Completion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plans.each do |plan| %>
      <tr>
        <td><%= plan.title %></td>
        <td><%= plan.user %></td>
        <td><%= plan.list %></td>
        <td><%= plan.completion %></td>
        <td><%= link_to 'Show', list_plan_path(@list, plan) %></td>
        <td><%= link_to 'Edit', edit_list_plan_path(@list, plan) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
